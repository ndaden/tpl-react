{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": [0],
        "indent": [0],
        "react/jsx-indent": [0],
        "react/button-has-type": [0],
        "react/jsx-filename-extension": [0],
        "import/prefer-default-export": [0],
        "linebreak-style": [0],
        "jsx-a11y/anchor-is-valid": [0]
    }
}